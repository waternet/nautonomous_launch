<launch>

	<arg name="gps_fix_topic" default="/sensor/gps/fix"/>
	<arg name="imu_topic" default="/sensor/imu/9dof"/>

	<arg name="operation_name_param" default="operation_name_param"/>

	<include file="$(find nautonomous_launch)/launch/nautonomous_template.launch">
		<arg name="operation_name_param" value="$(arg operation_name_param)"/>
		<arg name="simulate_group" value="true"/>
		<arg name="actuation_group" value="false"/>
		<arg name="webserver_group" value="false"/>
		<arg name="coordinate_simulate_group" value="true"/>
		<arg name="sensors_group" value="false"/>

		<arg name="gps_fix_topic" value="$(arg gps_fix_topic)"/>
		<arg name="imu_topic" value="$(arg imu_topic)"/>
	</include>

</launch>
